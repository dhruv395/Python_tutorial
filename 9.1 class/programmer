# Fields are name, salary, technologies.
## Create Getter and Setter method

class programmer:
    def setName(self,n):
        self.name=n
        
    def getName(self):
        return self.name
    
    def setSalary(self,sal):
        self.salary=sal
        
    def getSalary(self):
        return self.salary
    
    def setTechnologies(self,techs):
        self.technologies=techs
        
    def getTechnologies(self):
        return self.technologies
    
p1=programmer()
p1.setName("dhruv")
p1.setSalary(10000)
p1.setTechnologies(["java","python","c++"])

print(p1.getName())
print(p1.getSalary())
print(p1.getTechnologies())
