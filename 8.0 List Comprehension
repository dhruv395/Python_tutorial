## Calculate cubes of a given list
## normal method 

st=[]
for i in range(1,11):
    lst.append(i**3)
print(lst)

## using list comprehension

lst=[]
lst=[x**3 for x in range(1,11)]
print(lst)

###################################### find even nos using############################
lst=[x for x in range(2,21,2)]
print(lst)'''

lst=[x for x in range(1,21) if x%2==0]
print(lst)

######################################### product of List ##########################
a=[1,2,3,4,5]
b=[6,7,8,9,10]
z=[]

'''for i in range(len(a)):
    #print(a[i]*b[i])
    z.append(a[i]*b[i])
print(z)'''
##

z=[a[i]*b[i] for i in range(len(a))]
print(z)

############################################## Common Elements ##########################
a=[2,4,6,8]
b=[1,2,4,9]
result=[]

'''for i in a:
    if i in b:
        result.append(i)
print(result)'''

result=[i for i in a if i in b]
print(result)

