list=[10,20,"dhruv",30.05,-44]
print(list)
print(list[2])
print(list[2:5])
print(list*3)
print(len(list))


#add &remove a element in list
list.append(99)
print(list)

list.remove("dhruv")
print(list)

del(list[1])
print(list)

print(max(list))
print(min(list))

list.insert(1, 1000)
print(list)

list.sort()
print(list)

list.sort(reverse=True)
print(list)

list.clear()
print(list)
